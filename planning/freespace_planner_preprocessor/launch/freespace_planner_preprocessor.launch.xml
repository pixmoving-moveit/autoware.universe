<launch>
  <arg name="input_route" default="/planning/mission_planning/route" />
  <arg name="input_parking_state"
    default="/planning/scenario_planning/parking/freespace_planner/is_completed" />
  <arg name="input_odometry" default="/localization/kinematic_state" />
  <arg name="input_mission" default="/planning/scenario_planning/parking/freespace_planner/mission" />
  <arg name="input_planning_result"
    default="/planning/scenario_planning/parking/freespace_planner/planning_result" />
  <arg name="output_route" default="/planning/mission_planning/preprocess_route" />
  <arg name="output_parking_state" default="/planning/scenario_planning/parking/is_completed" />

  <arg name="config_path"
    default="$(find-pkg-share freespace_planner_preprocessor)/config/freespace_planner_preprocessor.param.yaml" />

  <node pkg="freespace_planner_preprocessor" exec="freespace_planner_preprocessor_node"
    name="freespace_planner_preprocessor" output="screen">
    <remap from="~/input/route" to="$(var input_route)" />
    <remap from="~/input/parking_state" to="$(var input_parking_state)" />
    <remap from="~/input/odometry" to="$(var input_odometry)" />
    <remap from="~/input/mission" to="$(var input_mission)" />
    <remap from="~/input/planning_result" to="$(var input_planning_result)" />
    <remap from="~/output/route" to="$(var output_route)" />
    <remap from="~/output/parking_state" to="$(var output_parking_state)" />

    <param from="$(var config_path)" />
  </node>
</launch>